{
  "name": "tab",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "pnpm run services:up && pnpm run services:wait:database && pnpm run migrations:up && next dev",
    "test": "pnpm run services:up && concurrently -n next,jest --hide next -k -s command-jest \"next dev\" \"jest --runInBand\"",
    "posttest": "pnpm run services:stop",
    "test:watch": "jest --runInBand --watchAll --verbose",
    "services:up": "docker-compose -f infra/compose.yaml up -d",
    "services:down": "docker-compose -f infra/compose.yaml down",
    "services:stop": "docker-compose -f infra/compose.yaml stop",
    "services:wait:database": "node infra/scripts/wait-for-postgres",
    "migrations:create": "node-pg-migrate --migrations-dir infra/migrations create",
    "migrations:up": "node-pg-migrate --migrations-dir infra/migrations --envPath .env.development up",
    "lint:pretirer:check": "prettier --check .",
    "lint:pretirer:fix": "prettier --write .",
    "lint:eslint:check": "next lint --dir .",
    "prepare": "husky",
    "commit": "cz"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.26.2",
  "dependencies": {
    "async-retry": "1.3.3",
    "cannon": "0.6.2",
    "dotenv": "16.4.5",
    "dotenv-expand": "12.0.3",
    "gsap": "~3.14.2",
    "lil-gui": "0.21.0",
    "next": "14.2.5",
    "node-pg-migrate": "7.6.1",
    "pg": "8.12.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "swr": "2.2.5",
    "three": "0.182.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.4.0",
    "@commitlint/config-conventional": "20.2.0",
    "commitizen": "4.3.1",
    "concurrently": "9.2.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jest": "28.8.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "prettier": "3.3.3",
    "sass": "1.97.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
